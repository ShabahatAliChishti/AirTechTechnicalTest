@model IEnumerable<AirTechTechnicalTest.Models.HomeSearching>


@foreach (var home in Model)
{
        <div class="col-md-4">
            <div class="single-latest-properties">
                <div class="property-img">
                    <img src="assets/img/listing1.jpg" alt="property image" />
                    <div class="property_overlay"></div>
                    <div class="pro-tag sale">
                        <p>for sale</p>
                    </div>
                    <div class="pro-info">
                        <ul>
                            <li>
                                <i class="fa fa-bed"></i>
                                @home.Bed
                            </li>
                            <li>
                                <i class="fa fa-bath"></i>
                                @home.Baths

                            </li>
                            <li>
                                <i class="fa fa-taxi"></i>
                                yes
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="property-text">
                    <h3><a href="#">@home.Office</a></h3>
                    <div class="pro-details-price">
                        <p>@home.Price</p>
                        <a href="#">More Details</a>
                    </div>
                </div>
            </div>
        </div>
        }
